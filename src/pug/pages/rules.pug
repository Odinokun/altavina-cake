extends ../_template

block variables
  - var title       = 'Alta Vina'
  - var description = 'Alta Vina'

block content
  .main-wrap.no-js
    include ../sections/header

    //begin content
    main.main.rules
      .container
        h2.rules__title.title
          span Правила акции

        //begin rules__list
        ol.rules__list.rules__list--master
          li
            ol.rules__list.rules__list--slave
              li Повседневная практика показывает, что постоянный количественный рост и сфера нашей активности играет важную роль в формировании направлений прогрессивного развития.
